<!DOCTYPE html>
<html>
  <head>
    <title>RetroBox</title>
    <%= csrf_meta_tags %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyABovltRtgUUgqDWu6VbLirL-nIU3itwUg" %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.retroBoxAssets = {
        logo: "<%= image_url("logo.png") %>",
        defaultThumb: "<%= image_url("default_thumbnail.jpeg") %>"
      };
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
